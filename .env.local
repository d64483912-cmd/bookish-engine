# Local development environment variables
# Copy from .env.example and update with your actual API keys

# AI Service Configuration
VITE_OPENROUTER_API_KEY=
VITE_OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# App Configuration
VITE_APP_NAME=Comet AI Browser
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-powered browser with smart automation

# Feature Flags
VITE_ENABLE_EXPERIMENTAL_FEATURES=false
VITE_ENABLE_ADVANCED_AI=false
VITE_ENABLE_CUSTOM_AGENTS=false

# Development
VITE_DEV_MODE=true